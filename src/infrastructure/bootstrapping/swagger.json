    "definitions": {
        "CreateTenantDto": {
            "required": ["name", "description"],
            "type": "object",
            "properties": {
                "name": {
                    "maxLength": 128,
                    "minLength": 0,
                    "type": "string"
                },
                "description": {
                    "maxLength": 256,
                    "minLength": 0,
                    "type": "string"
                }
            }
        },
        "TenantDto": {
            "required": ["description", "name"],
            "type": "object",
            "properties": {
                "id": {
                    "format": "int32",
                    "type": "integer"
                },
                "name": {
                    "maxLength": 128,
                    "minLength": 0,
                    "type": "string"
                },
                "description": {
                    "maxLength": 64,
                    "minLength": 0,
                    "pattern": "^[a-zA-Z][a-zA-Z0-9_-]{1,}$",
                    "type": "string"
                },
                "isActive": {
                    "type": "boolean"
                }
            }
        },
        "PagedResultDto[TenantDto]": {
            "type": "object",
            "properties": {
                "items": {
                    "uniqueItems": false,
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TenantDto"
                    }
                }
            }
        },
        "SignUpDto": {
            "required": ["name", "description"],
            "type": "object",
            "properties": {
              "firstName": {
                "maxLength": 50,
                "minLength": 0,
                "type": "string"
            },
            "lastName": {
                "maxLength": 50,
                "minLength": 0,
                "type": "string"
            },
            "email": {
                "maxLength": 128,
                "minLength": 0,
                "type": "string"
            },
            "username": {
                "maxLength": 100,
                "minLength": 0,
                "type": "string"
            },
            "password": {
                "maxLength": 128,
                "minLength": 0,
                "type": "string"
            }
            }
        },
        "userDto": {
            "required": [
                "firstName",
                "lastName",
                "email",
                "username",
                "password"
            ],
            "type": "object",
            "properties": {
                "firstName": {
                    "maxLength": 50,
                    "minLength": 0,
                    "type": "string"
                },
                "lastName": {
                    "maxLength": 50,
                    "minLength": 0,
                    "type": "string"
                },
                "email": {
                    "maxLength": 128,
                    "minLength": 0,
                    "type": "string"
                },
                "username": {
                    "maxLength": 100,
                    "minLength": 0,
                    "type": "string"
                },
                "id": {
                  "maxLength": 200,
                  "type": "string"
                }
            }
        },
        "PagedResultDto[userDto]": {
            "type": "object",
            "properties": {
                "userDto": {
                    "uniqueItems": false,
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/userDto"
                    }
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "SignInReqDto": {
          "required": ["emailOrUsername", "password"],
          "type": "object",
          "properties": {
          "emailOrUsername": {
              "maxLength": 100,
              "minLength": 0,
              "type": "string"
          },
          "password": {
              "maxLength": 128,
              "minLength": 0,
              "type": "string"
          }
          }
      },
      "PagedResultDto[SignInRespDto]": {
          "type": "object",
          "properties": {
              "token": {
                  "type": "string"
              }
          }
      }
    },
    "securityDefinitions": {
        "Bearer": {
            "name": "Authorization",
            "in": "header",
            "type": "apiKey",
            "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer {token}\""
        }
    },
    "security": [
